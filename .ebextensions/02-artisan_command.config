container_commands:
    01-no_dev:
        command: "composer.phar install --optimize-autoloader"
    02_cd_html:
        command: "cd /var/www/html"
    03_create_env:
        command: "/opt/elasticbeanstalk/bin/get-config environment | jq -r 'to_entries | .[] | "\(.key)=\"\(.value)\""' > .env"
    04-key_generate:
        command: "sudo php artisan key:generate"
    05-migrate:
        command: "sudo php artisan migrate --no-interaction --force"
    06-config_clear:
        command: "sudo php artisan config:clear"
    07-view_clear:
        command: "sudo php artisan view:clear"
    08-route_cache:
        command: "sudo php artisan route:cache"
    09-view_cache:
        command: "sudo php artisan view:cache"
    