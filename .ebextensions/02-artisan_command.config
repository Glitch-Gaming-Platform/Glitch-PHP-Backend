container_commands:
    01-no_dev:
        command: "composer.phar install --optimize-autoloader"
    02_cd_html:
        command: "cd /var/www/html"
    03_cd_html:
        command: "sudo cp .env.empty .env"
    04-migrate:
        command: "sudo php artisan migrate"
    05-key_generate:
        command: "sudo php artisan key:generate"
    06-config_clear:
        command: "sudo php artisan config:clear"
    07-view_clear:
        command: "sudo php artisan view:clear"
    08-route_cache:
        command: "sudo php artisan route:cache"
    09-view_cache:
        command: "sudo php artisan view:cache"
    